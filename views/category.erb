<div class="categoriesNav">
  <nav>
    <ul>
      <% while (@n < @categories.length) %>
        <% @categories.each do |category| %>
          <li><a href="/c/<%= @categories[@n] %>"><%= category %></a></li>
          <% @n += 1 %>
        <%end %>
      <%end %>
      <% @n = 0 %>
    </ul>
  </nav>
</div>
